<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dllo.keepproject">
    <!--网络权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--获取通讯录信息权限-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:name=".ui.app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/keep_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.WelcomeActivity"
            android:theme="@style/NoBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--欢迎页activity-->
        <activity
            android:name=".ui.activity.MainActivity" android:theme="@style/NoBar"/>

        <activity
            android:name=".ui.activity.FindSelectionsActivity"
            android:theme="@style/NoBar" />

        <!--这里是我的大图viewpager的界面-->
        <activity
            android:name=".ui.activity.BigMoveViewPageActivity"
            android:theme="@style/NoBar" />

        <!--这里是Findfragment,话题的二级界面-->
        <activity
            android:name=".ui.activity.TopicActivity"
            android:theme="@style/NoBar" />

        <!--这里是findfragment,点击下面的lv进入的动态详情二级界面-->
        <activity
            android:name=".ui.activity.DynamicSingleActivity"
            android:theme="@style/NoBar" />

        <!--没有用的-->
        <activity
            android:name=".ui.activity.DetailsActivity"
            android:theme="@style/NoBar" />

        <!--小组详情activity-->
        <activity
            android:name=".ui.activity.GroupDetailsActivity"
            android:theme="@style/NoBar" />

        <!--添加好友activity-->
        <activity
            android:name=".ui.activity.AddFriendActivity"
            android:theme="@style/NoBar" />

        <!--训练进去放视频的activity-->
        <activity
            android:name=".ui.activity.TrainVideoActivity"
            android:theme="@style/NoBar" />


        <!--这里是自定义相册的Activity-->
        <activity
            android:name=".ui.activity.PhotoAlbumActivity"
            android:theme="@style/NoBar" />

        <!--这里是自定义精选相册的Activity-->
        <activity
            android:name=".ui.activity.SelectPhotoActivity"
            android:theme="@style/NoBar" />

        <!--这里是自定义精选相册的详情的Activity-->
        <activity
            android:name=".ui.activity.SelectPhotoSingActivity"
            android:theme="@style/NoBar" />

        <!--选择地区activity-->
        <activity
            android:name=".ui.activity.CityActivity"
            android:theme="@style/NoBar" />

        <!--进入娱乐Activity-->
        <activity
            android:name=".ui.activity.EntertainmentActivity"
            android:theme="@style/NoBar" />

        <!--通讯录界面的Activity-->
        <activity
            android:name=".ui.activity.AddressBookActivity"
            android:theme="@style/NoBar" />


        <!--排行榜界面的Activity-->
        <activity
            android:name=".ui.activity.TopActivity"
            android:theme="@style/NoBar" />

        <!--这里是自定义相册的Activity-->
        <activity android:name=".ui.activity.PhotoAlbumActivity"
            android:theme="@style/NoBar"/>
        <!--这里是自定义精选相册的Activity-->
        <activity android:name=".ui.activity.SelectPhotoActivity"
            android:theme="@style/NoBar"/>
        <!--这里是自定义精选相册的详情的Activity-->
        <activity android:name=".ui.activity.SelectPhotoSingActivity"
            android:theme="@style/NoBar"/>
        <activity android:name=".ui.activity.CityActivity"
            android:theme="@style/NoBar"/>
        <activity android:name=".ui.activity.EntertainmentActivity"
            android:theme="@style/NoBar"/>
        <!--这里是热门搜索关键字的页面-->
        <activity android:name=".ui.activity.FindSeekActivity"
            android:theme="@style/NoBar"/>


        <!--消息界面的Activity-->
        <activity
            android:name=".ui.activity.NewsActivity"
            android:theme="@style/NoBar" />


        <!--新动态界面的Activity-->
        <activity
            android:name=".ui.activity.DynamicActivity"
            android:theme="@style/NoBar" />


        <!--点击图片显示照片大图-->
        <activity android:name=".ui.activity.ShowPhotoActivity"
            android:theme="@style/NoBar"/>
        <!--注册界面-->
        <activity android:name=".ui.activity.RegisterActivity"
            android:theme="@style/NoBar"/>
        <!--登录界面-->
        <activity android:name=".ui.activity.LoginActivity"
            android:theme="@style/NoBar"/>

        <!--设置界面-->
        <activity
            android:name=".ui.activity.SetUpActivity"
            android:theme="@style/NoBar" />

        <!--搜索界面-->
        <activity
            android:name=".ui.activity.SearchActivity"
            android:theme="@style/NoBar" />

        <!--个人资料-->
        <activity
            android:name=".ui.activity.PersonnalActivity"
            android:theme="@style/NoBar" />

        <!--音乐管理界面-->
        <activity android:name=".ui.activity.MusicManageActivity"
            android:theme="@style/NoBar"/>
    </application>

</manifest>